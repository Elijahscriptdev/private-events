<h1>Create Event</h1>

<%= form_for @event do |f| %>
<p>
  <%= f.label :event_name %>
  </p>
<p>
  <%= f.text_field :event_name %>
</p>
<p>
  <%= f.label :event_date %>
  </p>
<p>
  <%= f.date_field :event_date %>
</p>
<p>
  <%= f.label :description %>
  </p>
<p>
  <%= f.text_field :description %>
</p>
<p>
  <%= f.label :Location %>
  </p>
<p>
  <%= f.text_field :location %>
</p>
  <%= f.submit %>
<% end %>