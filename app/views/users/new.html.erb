<h1> SIGNUP </h1>

<h2> <%= link_to "Back", new_session_path %> </h2>

<%= form_for @user do |form| %>
    <p><%= form.label :username %></p>
    <p><%= form.text_field :username %></p>
    
    <p><%= form.label :password %></p>
    <p><%= form.password_field :password %></p>
    <p><%= form.submit %></p>
<% end %>