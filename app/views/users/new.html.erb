<h1> SIGNUP </h1>

<h1><%= link_to  "Go to Homepage", root_path %></h1>

<%= form_for @user do |f| %>
    
    <div>
        <%= f.label :username %>
        <%= f.text_field :username %>
    </div>

    <div>
        <%= f.label :email %>
        <%= f.email_field :email %>
    </div>
    
    <div>
        <%= f.label :password %>
        <%= f.password_field :password %>
    </div>
<% end %>