<h1> SIGNUP </h1>

<h1><%= link_to  "Go to Homepage", root_path %></h1>

<%= form_for @user do |f| %>
    
    <p> <%= f.label :username %></p>
    <p> <%= f.text_field :username %> </p>

    <p><%= f.label :email %></p>
    <p><%= f.email_field :email %></p>
    
    <p><%= f.label :password %></p>
    <p><%= f.password_field :password %></p>

    <p><%= f.submit %></p>
<% end %>